{
  "redirects": [
    {
      "source": "/newsletter/post/:slug(.*)",
      "destination": "/post/:slug",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "buildTimestamp": "2025-03-16T18:30:00Z",
  "installCommand": "npm install --legacy-peer-deps --no-audit",
  "buildCommand": "npm run build"
}
