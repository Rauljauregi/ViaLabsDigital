{
	"name": "blog-template",
	"type": "module",
	"version": "1.1.0",
	"description": "A template for creating a blog with Astro",
	"author": "danielcgilibert",
	"license": "GPL-3.0-only",
	"repository": {
		"type": "git",
		"url": "https://github.com/danielcgilibert/blog-template"
	},
	"keywords": [
		"astro",
		"astrojs",
		"astro-template",
		"astro-blog",
		"astro-b"
	],
	"bugs": {
		"url": "https://github.com/danielcgilibert/blog-template/issues"
	},
	"scripts": {
		"dev": "npx tinacms dev -c \"astro dev\"",
		"start": "astro dev",
		"build": "astro build",
		"sync": "astro sync",
		"preview": "astro preview",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/react": "^3.6.2",
		"@astrojs/rss": "4.0.1",
		"@astrojs/starlight": "^0.28.3",
		"@astrojs/starlight-docsearch": "^0.2.0",
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@giscus/react": "^3.0.0",
		"@mui/material": "^6.1.3",
		"@mui/system": "^6.1.3",
		"@mui/x-charts": "^7.20.0",
		"@nanostores/react": "^0.8.0",
		"algoliasearch": "^5.8.1",
		"astro": "^4.16.4",
		"astro-embed": "^0.7.4",
		"astro-font": "^0.0.72",
		"nanostores": "^0.11.3",
		"plotly.js": "^2.35.2",
		"react-loadable": "^5.5.0",
		"react-plotly.js": "^2.6.0",
		"rehype-katex": "^7.0.1",
		"remark-math": "^6.0.0"
	},
	"devDependencies": {
		"@astrojs/markdown-remark": "^5.3.0",
		"@astrojs/mdx": "2.0.3",
		"@astrojs/sitemap": "3.0.4",
		"@astrojs/tailwind": "5.1.0",
		"@garmin/fitsdk": "^21.141.0",
		"@pagefind/default-ui": "^1.0.4",
		"@shikijs/transformers": "^1.22.0",
		"@tailwindcss/typography": "0.5.10",
		"@tinacms/cli": "^1.5.31",
		"@typescript-eslint/parser": "^6.16.0",
		"clsx": "2.0.0",
		"crypto-js": "^4.2.0",
		"eslint": "^8.56.0",
		"eslint-plugin-astro": "^0.31.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"mdast-util-to-string": "^4.0.0",
		"motion": "^10.16.4",
		"pagefind": "^1.0.3",
		"prettier": "^3.0.3",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.12.0",
		"reading-time": "^1.5.0",
		"slugify": "^1.6.6",
		"tailwind-merge": "2.0.0",
		"tailwindcss": "3.3.5",
		"tinacms": "^1.5.22",
		"typescript": "^5.6.3",
		"unist-util-visit": "^5.0.0"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=."
		]
	}
}
